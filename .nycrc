{
  "check-coverage": true,
  "per-file": true,
  "lines": 99,
  "statements": 99,
  "functions": 99,
  "branches": 99,
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
  ],
  "reporter": [
    "text",
    "text-summary",
    "html"      
  ],
  "require": [
      "ts-node/register"
  ],
  "extension": [
    ".ts"
  ],
  "all": true,
  "report-dir": "./coverage"
}